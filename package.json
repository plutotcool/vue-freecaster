    "release": "changelogen --release --publish"
  },
  "peerDependencies": {
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@vitejs/plugin-vue": "^5.1.4",
    "changelogen": "^0.5.7",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.12",
    "vue-tsc": "^2.1.8"
  },
  "packageManager": "pnpm@9.0.0",
  "publishConfig": {
    "access": "public"
  }
}
